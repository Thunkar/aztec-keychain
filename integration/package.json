{
  "name": "@thunkar/aztec-keychain-accounts",
  "homepage": "https://github.com/Thunkar/aztec-keychain",
  "description": "TS implementation for the aztec-keychain account wallet",
  "version": "0.1.2",
  "type": "module",
  "exports": {
    "./ecdsa": "./dest/ecdsa/index.js"
  },
  "typedocOptions": {
    "entryPoints": [
      "./src/ecdsa/index.ts"
    ],
    "name": "aztec-keychain-accounts",
    "tsconfig": "./tsconfig.json"
  },
  "scripts": {
    "build": "yarn clean && tsc -b",
    "build:dev": "tsc -b --watch",
    "build:ts": "tsc -b",
    "clean": "rm -rf ./dest tsconfig.tsbuildinfo",
    "formatting": "prettier --check ./src && run -T eslint ./src",
    "formatting:fix": "eslint --fix ./src && run -T prettier -w ./src"
  },
  "dependencies": {
    "@aztec/accounts": "0.85.0-alpha-testnet.5",
    "@aztec/aztec.js": "0.85.0-alpha-testnet.5",
    "@aztec/stdlib": "0.85.0-alpha-testnet.5",
    "pako": "^2.1.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/node": "^18.7.23",
    "@types/pako": "^2.0.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "files": [
    "dest",
    "src",
    "!*.test.*",
    "artifacts"
  ],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22"
}
